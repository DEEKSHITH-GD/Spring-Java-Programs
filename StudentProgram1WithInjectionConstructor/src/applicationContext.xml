<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
       
       <bean id="stud" class="com.studConstructor.Student">
       		<constructor-arg type="int" name="id">
       			<value>1</value>
       		</constructor-arg>
       		<constructor-arg type="String" name="name">
       			<value>Deekshith</value>
       		</constructor-arg>
       		<constructor-arg type="String" name="college">
       			<value>SCE</value>
       		</constructor-arg>
       		<constructor-arg type="String" name="usn">
       			<value>1SG19CS111</value>
       		</constructor-arg>
       		<constructor-arg type="int" name="age">
       			<value>22</value>
       		</constructor-arg>
       </bean>         
</beans>